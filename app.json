{
  "expo": {
    "name": "books",
    "slug": "books",
    "scheme": "books",
    "version": "1.0.0",
    "android": {
      "googleServicesFile": "./google-services.json",
      "package": "com.books.app"
    },
    "ios": {
      "googleServicesFile": "./GoogleService-Info.plist",
      "bundleIdentifier": "com.books.app"
    },
    "plugins": [
      "@react-native-firebase/app",
      "expo-router",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      "expo-asset",
      [
        "expo-font",
        {
          "fonts": ["./src/assets/fonts/georgia.ttf", "./src/assets/fonts/nunito-sans.ttf"]
        }
      ]
    ]
  }
}
